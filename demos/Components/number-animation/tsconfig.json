
{
  "include": [
    "src/**/*.ts",
    "index.d.ts",
    "index.d.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es2015",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}

